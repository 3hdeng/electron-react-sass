{
  "name": "electron-react-sass",
  "version": "1.0.0",
  "description": "A minimal Electron application for react + sass",
  "main": "main.js",
  "scripts": {
    "start": "set ENVIRONMENT=DEV&& electron  main.js",
    "build": "./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack-dev-server --hot --inline",
    "babel": "babel src -d lib",
    "sass": "node-sass --watch src/stylesheets/main.scss  dist/stylesheets/main.css"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "electron-prebuilt": "^0.37.0",
    "jsx-loader": "^0.13.2",
    "lodash": "^3.9.3",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.7.0",
    "react": "^15.0.2",
    "react-addons-css-transition-group": "^15.0.2",
    "react-dom": "^15.0.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "moment": "^2.10.3"
  }
}
