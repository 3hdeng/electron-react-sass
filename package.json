{
  "name": "electron-react-sass",
  "version": "1.0.0",
  "description": "A minimal Electron application for react + sass",
  "main": "main.js",
  "scripts": {
    "start": "set ENVIRONMENT=DEV&& electron  main.js",
    "build": "./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack-dev-server --hot --inline",
    "babel": "babel src -d lib"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.5.0",
    "electron-prebuilt": "^0.37.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
